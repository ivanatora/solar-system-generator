<% include header %>

    <table class="table table-striped table-hover catalog">
        <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Type</th>
                <th>Planets</th>
            </tr>
        </thead>
        <tbody>
            <% for (i = 0; i < data.length; i++){ %>
            <tr>
                <td><%= i+1 %></td>
                <td><%= data[i].name %></td>
                <td><%= data[i].class %></td>
                <td>Column content</td>
            </tr>
            <% } %>
            
        </tbody>
    </table>

<% include footer %>